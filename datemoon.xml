        <script type="text/javascript">

            function showSaint(day)
            {
            var ctime = new Date();
            var pmonth = ctime.getMonth();
            var pday = ctime.getDate();
            if (day <= 1)
            day = 1;
            if (day >= 31)
            day = 31;
            var str = getSaint(day, pmonth+1);
            _gel('gdt_saint___MODULE_ID__').innerHTML = getSaint(((str == "undefined") ? pday : day), pmonth+1);

            if (this.is_ie5 || this.is_ie6) {
            fixIeBkgPng2('gdt_clock___MODULE_ID__', CalcPhase(day), 'no-repeat');
            } else {
            setBkgPng2('gdt_clock___MODULE_ID__', CalcPhase(day), 'no-repeat');
            }
            }

            function fixIeBkgPng2(elId, imgSrc, repeat) {
            _gel(elId).style.filter = 'progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled=true, src=\''+imgSrc+'\')';
            _gel(elId).style.backgroundImage = 'none';
            }

            function setBkgPng2(elId, imgSrc, repeat) {
            _gel(elId).style.backgroundImage = 'url(\'' + imgSrc + '\')';
            _gel(elId).style.backgroundRepeat = repeat;
            }


